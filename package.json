{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "test": "umi test",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src mock",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx src mock --fix",
    "prettier": "check-prettier lint",
    "prettier-fix": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "ant-design-pro": "^2.2.1",
    "antd": "^3.13.6",
    "dva": "^2.5.0-beta.2",
    "enquire.js": "^2.1.6",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-eslint": "^9.0.0",
    "check-prettier": "^1.0.1",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.4.2",
    "umi-plugin-react": "^1.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/prettier.js",
      "git add"
    ],
    "src/**/*.{js,ts,tsx,jsx}": [
      "eslint --ext .js,.jsx,.ts,.tsx src mock --fix",
      "git add"
    ]
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "scripts/**/*.js"
  ],
  "engines": {
    "node": ">=8.0.0"
  }
}
